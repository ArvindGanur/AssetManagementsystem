    <div class="form-group row">

        <div class="col-2">
            <label asp-for="AssetIssueDetails.Fdx_Emp_Id"></label>
        </div>
        <div class="col-2">
            <input asp-for="AssetIssueDetails.Fdx_Emp_Id" class="form-group" />
        </div>
        <div class="col-2">
            <label asp-for="AssetIssueDetails.Fdx_Emp_Name"></label>
        </div>
        <div class="col-2">
            <input asp-for="AssetIssueDetails.Fdx_Emp_Name" class="form-group" />
        </div>
        <div class="col-2">
            <label asp-for="AssetIssueDetails.Fdx_Emp_Designation"></label>
        </div>
        <div class="col-2">
            <input asp-for="AssetIssueDetails.Fdx_Emp_Designation" class="form-group" />
