        </div>
        <div class="col-2">
            <input asp-for="AssetIssueDetails.Fdx_Emp_Acknowledge" class="form-group" />
        </div>
        <div class="col-2">
            <label asp-for="AssetIssueDetails.Fdx_Emp_Replace_Item"></label>
        </div>
        <div class="col-2">
            <input asp-for="AssetIssueDetails.Fdx_Emp_Replace_Item" class="form-group" />
        </div>
        <div class="col-2">
            <label asp-for="AssetIssueDetails.Fdx_Asset_no"></label>
        </div>
        <div class="col-2">
            @*<select asp-for="AssetIssueDetails.Fdx_Asset_no" asp-items="Model.CselectListItem(Model.AssetDetails)" class="form-control"></select>*@
            <input asp-for="AssetIssueDetails.Fdx_Asset_no" class="form-group" />
