        <div class="col-2">
            <input asp-for="AssetIssueDetails.Fdx_Asset_Remarks" class="form-group" />
        </div>


        <div class="form-group">
            <input type="submit" class="btn btn-primary" value="Create" />
            <a asp-controller="Issue" asp-action="Index" class="btn btn-success">Back</a>
        </div>

    </div>
