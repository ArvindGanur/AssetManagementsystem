@model AssetManagementSystemFidrox.Models.ViewModels.AssetViewModel
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<br />
<br />

<h2 class="text-info">Add New Asset Entry</h2>
